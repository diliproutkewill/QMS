<?xml version="1.0"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>


<weblogic-rdbms-jar>

  <weblogic-rdbms-bean>
    <ejb-name>DesignationBean</ejb-name>
    <data-source-name>oraclePool</data-source-name>
	 <table-map>
    <table-name>QMS_DESIGNATION</table-name>
    <field-map>
      <cmp-field>designationId</cmp-field>
      <dbms-column>DESIGNATION_ID</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>description</cmp-field>
      <dbms-column>DESCRIPTION</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>levelNo</cmp-field>
      <dbms-column>LEVEL_NO</dbms-column>
    </field-map>
    <field-map>
      <cmp-field>invalidate</cmp-field>
      <dbms-column>INVALIDATE</dbms-column>
    </field-map>
	</table-map>
  </weblogic-rdbms-bean>

  <weblogic-rdbms-bean>
  <ejb-name>DensityGroupCodeBean</ejb-name>
  <data-source-name>oraclePool</data-source-name>
  <table-map>
  <table-name>QMS_DENSITY_GROUP_CODE</table-name>
  <field-map>
	<cmp-field>dgcCode</cmp-field>
	<dbms-column>DGCCODE</dbms-column>
  </field-map>
  <field-map>
	<cmp-field>uom</cmp-field>
	<dbms-column>UOM</dbms-column>
  </field-map>
  <field-map>
	<cmp-field>perKG</cmp-field>
	<dbms-column>KG_PER_M3</dbms-column>
  </field-map>
  <field-map>
	<cmp-field>perLB</cmp-field>
	<dbms-column>LB_PER_F3</dbms-column>
  </field-map>
   <field-map>
	<cmp-field>invaliDate</cmp-field>
	<dbms-column>INVALIDATE</dbms-column>
  </field-map>
  </table-map>
     <weblogic-query>
     <description>find all acounts balance is greater than 10000</description>
      <query-method>
        <method-name>findAll</method-name>
		<method-params/>
      </query-method>
    </weblogic-query>
  </weblogic-rdbms-bean>
</weblogic-rdbms-jar>
