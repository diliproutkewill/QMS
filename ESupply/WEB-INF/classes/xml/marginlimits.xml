			<size>10</size>
			<maxLength>15</maxLength>
			<class>text</class>
			<onKeyPress></onKeyPress>
			<onBlur>return checkNumbersAndRoundToTwoDecimal(this,"maxDiscount");toUpper(this)</onBlur>
		</ChildElement>
		<ChildElement elementType="input"  class="formdata" align="center">
			<type>text</type>
			<name>minMargin</name>
