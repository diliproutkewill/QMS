<DynamicContent>
	<marginlimit>
		<ChildElement elementType="input" startingElement="true" class="formdata" align="center">
			<type>text</type>
			<name>levelId</name>
			<size>10</size>
			<maxLength>3</maxLength>
			<class>text</class>
			<onKeyPress></onKeyPress>
			<onBlur>specialCharFilter(this,"levelId");toUpper(this);</onBlur>
		</ChildElement>
		<ChildElement elementType="input" merged="true" class="formdata" align="center">
			<type>button</type>
			<name>levelb1</name>
			<class>input</class>
			<onClick>openLevelIdLOV(this)</onClick>
			<value>...</value>
		</ChildElement>
		<ChildElement elementType="input" startingElement="true" class="formdata" align="center">
			<type>text</type>
			<name>serviceLevelId</name>
