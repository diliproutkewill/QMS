			<class>text</class>
			<onKeyPress></onKeyPress>
			<onBlur>specialCharFilter(this,"serviceLevelId");toUpper(this);</onBlur>
		</ChildElement>
		<ChildElement elementType="input" merged="true" class="formdata" align="center">
			<type>button</type>
			<name>sLevelb1</name>
